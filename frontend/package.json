{
  "name": "react-cognito-cms-boilerplate",
  "version": "0.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Vic-L/react-cognito-cms-boilerplate"
  },
  "description": "Boilerplate for react application",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=development node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build:prod": "NODE_ENV=production webpack -p --config webpack.config.js",
    "deploy": "npm run build:prod && npm run deploy:remove-assets && npm run deploy:upload-assets",
    "deploy:remove-assets": "aws s3 rm $assetsDirectory --recursive --profile $awsProfile",
    "deploy:upload-assets": "aws s3 cp ./dist/assets $assetsDirectory --recursive --acl public-read --exclude '*.DS_Store' --profile $awsProfile --cache-control public,max-age=86400"
  },
  "author": "Vic-L",
  "license": "ISC",
