{
  "name": "webpack_react_boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for react application",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "NODE_ENV=development node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "build:prod": "NODE_ENV=production webpack -p --config webpack.config.js",
    "deploy": "NODE_ENV=production webpack -p --config webpack.config.js && aws s3 rm s3://TODO_BUCKET_NAME/assets --recursive --profile TODO_AWS_PROFILE_NAME && aws s3 cp ./dist s3://TODO_BUCKET_NAME  --recursive --acl public-read --exclude '*.DS_Store' --profile TODO_AWS_PROFILE_NAME --cache-control public,max-age=86400"
  },
